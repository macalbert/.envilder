{
  "name": "envilder-alpha",
  "version": "0.1.6",
  "main": "index.js",
  "description": "A CLI tool to generate .env files from AWS SSM parameters",
  "bin": {
    "envilder": "./dist/envilder.ts"
  },
  "scripts": {
    "build": "tsc",
    "package": "pkg dist/envilder.js --out-path dist",
    "publish": "npm run build && npm run package && npm version patch && npm publish",
    "test": "jest --no-cache --env=node --colors --seed=1 --coverage=true",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|tsx)\""
  },
  "keywords": [],
  "author": "Mar√ßal",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.5.5",
    "aws-sdk": "^2.1691.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "esbuild": "^0.23.1",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "pkg": {
    "scripts": "dist/envilder.js",
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
